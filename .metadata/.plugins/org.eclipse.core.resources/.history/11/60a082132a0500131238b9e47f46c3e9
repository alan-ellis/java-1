/*
null<-[1]-><-[2]->null
*/
public class DoubleLinkList {
	public static class Node{
		int data;
		Node pre;
		Node next;
		
		Node(int d){
			this.data = d;
			pre = null;
			next = null;
		}
	}
	
	Node head;
	Node tail;
	int length;
	
	public void addToTail(int d){
		if (this.head == null){
			head = new Node(d);
			tail = head;
		} else {
			tail.next = new Node(d);
			tail.next.pre = tail;
			tail = tail.next;
		}
		this.length++;
	}
	
	public void addToHead(int d){
		if (this.head == null){
			head = new Node(d);
			tail = head;
		} else {
			head.pre = new Node(d);
			head = head.pre;
		}
		this.length++;
	}
	
	public void insert(int key, int newKey){
		
	}
	
	public int getAtIndex(int i){
		
	}
	
}
