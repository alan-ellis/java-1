
public class LinkList {
	public class Node{
		int data;
		Node next;
		
		Node(int d){
			this.data = d;
			this.next = null;
		}
	}
	
	Node head;
	Node tail;
	int length;

	LinkList(){
		this.head = null;
		this.tail = null;
		this.length = 0;
	}
	public void addToTail(int d){
		if (head == null) {
			head = new Node(d);
			tail = head;
		} else {
			tail.next = new Node(d);
			tail = tail.next;
		}
		length++;
	}
}
